<% include partial/head %>
<% include partial/header %>

<div class="header post-header">
<h1 class="post-title"><a href="#"><%- article.title %></a></h1>
    <p class="post-meta">
        <%- article.date %>. 
        By <a href="#" class="post-author"><%- article.author %></a> under 
        <% article.categories.forEach(function(category) { %>
            <a class="post-category" href="#"><%- category %></a> 
        <% }) %>
    </p>
</div>

<div class="content">

    <%- contents %>
            
</div>
<% include partial/footer %>
<% include partial/end %>